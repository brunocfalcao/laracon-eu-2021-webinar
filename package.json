{
    "private": true,
    "scripts": {
        "dev": "npm run development",
        "development": "cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --progress --config=node_modules/laravel-mix/setup/webpack.config.js",
        "watch": "npm run development -- --watch",
        "watch-poll": "npm run watch -- --watch-poll",
        "hot": "cross-env NODE_ENV=development node_modules/webpack-dev-server/bin/webpack-dev-server.js --inline --hot --disable-host-check --config=node_modules/laravel-mix/setup/webpack.config.js",
        "prod": "npm run production",
        "production": "cross-env NODE_ENV=production node_modules/webpack/bin/webpack.js --no-progress --config=node_modules/laravel-mix/setup/webpack.config.js",
        "build-incidents-insights": "cd nova-components/IncidentsInsights && npm run dev",
        "build-incidents-insights-prod": "cd nova-components/IncidentsInsights && npm run prod",
        "build-incidents-total": "cd nova-components/IncidentsTotal && npm run dev",
        "build-incidents-total-prod": "cd nova-components/IncidentsTotal && npm run prod",
        "build-total-incidents": "cd nova-components/TotalIncidents && npm run dev",
        "build-total-incidents-prod": "cd nova-components/TotalIncidents && npm run prod",
        "build-incidents-total-card": "cd nova-components/IncidentsTotalCard && npm run dev",
        "build-incidents-total-card-prod": "cd nova-components/IncidentsTotalCard && npm run prod",
        "build-my-total-tickets-card": "cd nova-components/MyTotalTicketsCard && npm run dev",
        "build-my-total-tickets-card-prod": "cd nova-components/MyTotalTicketsCard && npm run prod",
        "build-my-total-incidents-card": "cd nova-components/MyTotalIncidentsCard && npm run dev",
        "build-my-total-incidents-card-prod": "cd nova-components/MyTotalIncidentsCard && npm run prod",
        "build-fa-field": "cd nova-components/FaField && npm run dev",
        "build-fa-field-prod": "cd nova-components/FaField && npm run prod",
        "build-fa-text": "cd nova-components/FaText && npm run dev",
        "build-fa-text-prod": "cd nova-components/FaText && npm run prod",
        "build-my-total-incidents-async-card": "cd nova-components/MyTotalIncidentsAsyncCard && npm run dev",
        "build-my-total-incidents-async-card-prod": "cd nova-components/MyTotalIncidentsAsyncCard && npm run prod",
        "build-universal-blade-tool": "cd nova-components/UniversalBladeTool && npm run dev",
        "build-universal-blade-tool-prod": "cd nova-components/UniversalBladeTool && npm run prod"
    },
    "devDependencies": {
        "axios": "^0.19",
        "cross-env": "^7.0",
        "laravel-mix": "^5.0.1",
        "lodash": "^4.17.19",
        "resolve-url-loader": "^3.1.0",
        "vue-template-compiler": "^2.6.12"
    }
}